{
  "name": "stocks",
  "version": "1.0.0",
  "description": "stocks market information",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodmon server.js",
    "start": "if-env NODE_ENV=production && npm start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm run install",
    "build": "cd client/ && npm install && num run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hatout1/stocks.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hatout1/stocks/issues"
  },
  "homepage": "https://github.com/hatout1/stocks#readme",
  "dependencies": {
    "bcrypt": "^4.0.1",
    "concurrently": "^5.1.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-cli": "0.0.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.7",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "request": "^2.88.2"
  }
